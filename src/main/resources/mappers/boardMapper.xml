<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace = "com.jiocompany.crudBoard.boardMapper">
	
	<!-- 글 전체 목록 -->
	<select id="board" resultType="BoardDTO">
	select *
	from board
	where b_del = 0 
	order by b_no
	</select>
	
	
	
</mapper>






